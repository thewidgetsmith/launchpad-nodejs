{
  "root": true,
  "env": {
    "node": true,
    "es6": true
  },
  "rules": {
    "no-console": 1,
    "eqeqeq": "warn",
    "no-unused-vars": 1,
    "no-extra-semi": "warn",
    "semi": "off"
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  }
}
